---
import Navbar from '@components/Navbar.astro';
export interface IBaseHeader {
	arrowBack: boolean;
	share: boolean;
	close: boolean;
	search: boolean;
	menu: boolean;
}

const { arrowBack, share, close, search, menu } = Astro.props as IBaseHeader;
---

<header>
	<Navbar
		arrowBack={arrowBack}
		share={share}
		close={close}
		search={search}
		menu={menu}
	/>
</header>
