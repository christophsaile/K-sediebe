{
  "name": "kaesediebe",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build",
    "build:search": "node ./scripts/buildSearch.js",
    "build:types": "contentful-typescript-codegen --output src/types/generated/contentful.ts",
    "build:pwa-assets": "npx pwa-asset-generator logo.png ./icons -b '#f3f4f7' -w",
		"build:prod": "astro build && npm run sw && npm run build:search",
    "preview": "astro preview",
    "lint": "npx prettier --write src/**/*",
    "sw": "npx workbox-cli generateSW workbox-config.js "
  },
  "devDependencies": {
    "@astrojs/solid-js": "^0.2.0",
    "@astrojs/tailwind": "^0.2.1",
    "astro": "1.0.3",
    "contentful": "^9.1.32",
    "contentful-management": "^10.6.3",
    "contentful-typescript-codegen": "^3.2.3",
    "dotenv": "^16.0.1",
    "prettier": "^2.7.1",
    "prettier-plugin-astro": "^0.1.1",
    "solid-js": "^1.4.4"
  },
  "dependencies": {
    "@contentful/rich-text-html-renderer": "^15.13.1",
    "@contentful/rich-text-types": "^15.12.1",
    "algoliasearch": "^4.14.2",
    "daisyui": "^2.17.0"
  }
}
