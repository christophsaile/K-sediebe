{
	"name": "kaesediebe",
	"version": "1.0.0",
	"private": true,
	"scripts": {
		"astro": "astro",
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro build",
		"build:search": "node ./scripts/buildSearch.js",
		"build:types": "contentful-typescript-codegen --output src/types/generated/contentful.ts",
